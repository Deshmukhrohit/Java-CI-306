import java.util.Scanner;

public class Main
{
    public static void main(String args[])
    {
        int num, i, fact=1;
        Scanner scan = new Scanner(System.in);
      
        System.out.print("Enter a Number : ");
        num = scan.nextInt();
      
        for(i=1; i<=num; i++)
        {
            fact=fact*i;
        }
          System.out.print("Factor of "+num+" is"+fact);
        }
    }
