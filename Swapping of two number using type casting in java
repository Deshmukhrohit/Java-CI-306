public class swap{
    public static void main(String[] args){
        int a = 59 ,b = 43;
        int c;
        c = a;
        a = b;
        b = c;
        double d = a;
        double s = b;
        System.out.println(a);
        System.out.println(b);
        System.out.println (d);
        System.out.println (s);
        
    }
}
